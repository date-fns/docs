{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "skipLibCheck": true,
    "outDir": "lib",
    "sourceMap": true,
    "declaration": true
  }
}
